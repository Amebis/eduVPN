<view:App
    x:Class="LetsConnect.Client.App"
    xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
    xmlns:view="clr-namespace:eduVPN.Views;assembly=eduVPN.Views"
    StartupUri="/eduVPN.Views;component/Windows/ConnectWizard.xaml">

    <Application.Resources>
        <ResourceDictionary>
            <ResourceDictionary.MergedDictionaries>
                <ResourceDictionary Source="pack://application:,,,/eduVPN.Views;component/Resources/Constants.xaml"/>

                <ResourceDictionary Source="pack://application:,,,/eduVPN.Views;component/Resources/Converters.xaml"/>

                <ResourceDictionary Source="Resources/Colors.xaml"/>
                <ResourceDictionary Source="Resources/Images.xaml"/>
                <ResourceDictionary Source="pack://application:,,,/eduVPN.Views;component/Resources/Icons.xaml"/>

                <ResourceDictionary Source="pack://application:,,,/eduVPN.Views;component/Resources/Styles.xaml"/>
                <ResourceDictionary Source="pack://application:,,,/eduVPN.Views;component/Resources/DataTemplates.xaml"/>
            </ResourceDictionary.MergedDictionaries>
        </ResourceDictionary>
    </Application.Resources>
</view:App>
